{
  "manifest_version": 2,
  "description": "An extension to help you manage your messy bookmarks!",
  "name": "bookmarker",
  "version": "1.0",
  "permissions": ["tabs"],
  "content_scripts": [
      {
      "matches": ["<all_urls>"]
    }
  ],
  "browser_action": {
    "default_popup": "dist-app/app.html"
  }   
}
